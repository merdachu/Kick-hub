local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("KICK HUB", "Sentinel")
  -- MAIN
  local Main = Window:NewTab("Main")
  local MainSection = Main:NewSection("Main")
  MainSection:NewDropdown("Blox fruits", "StrikeHub", {"Execute"}, function(v)
    getgenv().Color = Color3.fromRGB(0, 128, 255)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/StormSKz12/StirkeHub1/main/Gameincluded"))()
  end)
  MainSection:NewDropdown("King Legacy", "BKHAX", {"Execute"}, function(v)
    loadstring(game:HttpGet(("https://raw.githubusercontent.com/koonpeatch/PeatEX/master/BKHAX/BKHAXHUB"),true))()
  end)

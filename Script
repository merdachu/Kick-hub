local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("KICK HUB", "Sentinel")

  -- MAIN
  local Main = Window:NewTab("Main")
  local MainSection = Main:NewSection("Main")
  MainSection:NewDropdown("Blox fruits", "xDepressionx", {"Execute"}, function(v)
loadstring(game:HttpGet("https://raw.githubusercontent.com/xDepressionx/Free-Script/327527e41efc3fd0a46a2cdff649957295a00014/BloxFruit.lua"))()
  end)
  MainSection:NewDropdown("King Legacy", "BKHAX", {"Execute"}, function(v)
    loadstring(game:HttpGet(("https://raw.githubusercontent.com/koonpeatch/PeatEX/master/BKHAX/BKHAXHUB"),true))()
  end)
  MainSection:NewDropdown("GPO", "JakeKill", {"Execute"}, function(v)
    loadstring(game:HttpGet(("https://raw.githubusercontent.com/Jakekill871/public-scripts/master/GrandPieceOnlineGUI"),true))()
  end)
  MainSection:NewDropdown("Da hood", "Inceldom", {"Execute"}, function(v)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/inceldom/dh/main/script",Atom2IsSkidded==EvenTheWhitelist))()
  end)
  MainSection:NewDropdown("Anime fighters", "DollHouse", {"Execute"}, function(v)
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/pspboy08/dollhouse/main/AFSGUI.lua')))()
  end)

  --PLAYER
  local Player = Window:NewTab("Player")
  local PlayerSection = Player:NewSection("Player")

  PlayerSection:NewSlider("Walkspeed", "Changes the walkspeed", 250, 16, function(v)
      game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
  end)

  PlayerSection:NewSlider("Jumppower", "Changes the jumppower", 250, 50, function(v)
      game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
  end)

  --Settings
  local Settings = Window:NewTab("Settings")
  local SettingsSection = Settings:NewSection("Settings")

  SettingsSection:NewKeybind("Toggle UI", "Sets the key to toggle the ui", Enum.KeyCode.RightShift, function(v)
    Library:ToggleUI()
  end)
